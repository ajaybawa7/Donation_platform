<h2>All Donations</h2>
<table>
    <tr>
        <th>Donor Name</th>
        <th>Campaign</th>
        <th>Amount</th>
        <th>Date</th>
    </tr>
    <% donations.forEach(donation => { %>
        <tr>
            <td><%= donation.donor_name %></td>
            <td><%= donation.campaign_name %></td>
            <td>$<%= donation.amount %></td>
            <td><%= donation.donation_date %></td>
        </tr>
    <% }) %>
</table>
